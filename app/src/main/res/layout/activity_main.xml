<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:text="BIENVENIDOS A NUESTRA APLICACION!"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="68dp" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="ENCUENTRA LUGARES ESPECTACULARES"
        android:textSize="13dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="203dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/img10"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="133dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/hotel"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="343dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="encuentra con nosotros
         los mejores lugares"
        android:textSize="13dp"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="408dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="380dp"
        android:layout_height="80dp"
        android:text="Buscar lugares"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="564dp" />


</androidx.constraintlayout.widget.ConstraintLayout>